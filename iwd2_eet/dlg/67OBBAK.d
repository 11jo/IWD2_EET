BEGIN ~67OBBAK~

IF WEIGHT #3
~NumTimesTalkedTo(0)~ THEN BEGIN 0
  SAY @32450
  IF ~~ THEN REPLY @32451 EXIT
  IF ~~ THEN REPLY @32452 DO ~Enemy()~ EXIT
END

IF WEIGHT #2
~NumTimesTalkedToGT(0)~ THEN BEGIN 1
  SAY @32453
  IF ~~ THEN REPLY @32451 EXIT
  IF ~~ THEN REPLY @32454 GOTO 2
  IF ~~ THEN REPLY @22283 DO ~Enemy()~ EXIT
END

IF ~~ THEN BEGIN 2
  SAY @32455
  IF ~~ THEN REPLY @32456 EXIT
  IF ~~ THEN REPLY @32457 DO ~Enemy()~ EXIT
END

IF WEIGHT #1
~Global("SH_Stop_Envoys","GLOBAL",1)~ THEN BEGIN 3
  SAY @32458
  IF ~~ THEN REPLY @32459 EXIT
  IF ~~ THEN REPLY @32452 DO ~Enemy()~ EXIT
END

IF WEIGHT #0
~Global("SH_Stub_Pissed","GLOBAL",1)~ THEN BEGIN 4
  SAY @35288
  IF ~~ THEN EXIT
END
