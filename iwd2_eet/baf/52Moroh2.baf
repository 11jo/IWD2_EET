	GlobalGT("C2_MONK_DEAD","GLOBAL",0)
	Global("CHAMBER_2_MONK_CNT","MYAREA",0)
THEN
	RESPONSE #100
		SetGlobal("C2_MONK_DEAD","GLOBAL",0)
		Continue()
END

IF
	GlobalLT("CHAMBER_2_MONK_CNT","MYAREA",5)
	GlobalGT("CHAMBER_2_MONK_REQUEST","MYAREA",0)
	!Global("WATCHING_PLAYER","MYAREA",0)
	Global("52Morohem_Master","GLOBAL",2)
THEN
	RESPONSE #100
		CreateCreature("52MNKSH",[1086.1389],2)
		//SpellHitEffectSprite(LastMarkedObject,LastMarkedObject,DEATH_SPELL,0)
		CreateVisualEffectObject("DSPELLH","52MNKSH")
		//HideCreature(LastMarkedObject,FALSE)
		ActionOverride("52MNKSH",ApplySpellRES("idinvis5",Myself))
		IncrementGlobal("CHAMBER_2_MONK_REQUEST","MYAREA",-1)
		IncrementGlobal("CHAMBER_2_MONK_CNT","MYAREA",1)
END

IF
	Global("C_2_MASTERED","MYAREA",0)
	Global("WATCHING_PLAYER","MYAREA",1)
	Global("CHAMBER_2_TERROR_LEVEL","MYAREA",4)
THEN
	RESPONSE #100
		StartCutSceneMode()
		SetGlobal("C_2_MASTERED","MYAREA",1)
		ClearAllActions()
		MultiPlayerSync()
		Wait(1)
		FadeToColor([0.0],0)
		Wait(1)
		//DropInventoryEX(Player1)
		ActionOverride(Player1,DropInventory())
		ActionOverride(Player1,ApplySpell(Myself,DISPEL_NO_SAVE))
		MultiPlayerSync()
		SetGlobal("WATCHING_PLAYER","MYAREA",0)
		ActionOverride(Player1,JumpToObject("Target")) 
		MultiPlayerSync()
		SetGlobal("52Morohem_Master","GLOBAL",3)
		Unlock("AR5202_Door3")
		OpenDoor("AR5202_Door3")
		AddXP2DA("IW2EX10A")
		DisplayStringNoName(Player1,@34471)
		Wait(1)
		/*ChangeEnemyAlly(Player2,PC)
		ChangeEnemyAlly(Player3,PC)
		ChangeEnemyAlly(Player4,PC)
		ChangeEnemyAlly(Player5,PC)
		ChangeEnemyAlly(Player6,PC)*/
		ActionOverride(Player2,ApplySpellRES("IDRUNSEL",Myself))
		ActionOverride(Player3,ApplySpellRES("IDRUNSEL",Myself))
		ActionOverride(Player4,ApplySpellRES("IDRUNSEL",Myself))
		ActionOverride(Player5,ApplySpellRES("IDRUNSEL",Myself))
		ActionOverride(Player6,ApplySpellRES("IDRUNSEL",Myself))
		MultiPlayerSync()
		//ResetJoinRequests()
		MultiPlayerSync()
		MoveViewObject(Player1,INSTANT)
		MultiPlayerSync()
		MultiPlayerSync()
		FadeFromColor([0.0],0)
		Wait(1)
		MultiPlayerSync()
		EndCutSceneMode()
END

