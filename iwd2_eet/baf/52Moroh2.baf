IF
	GlobalGT("C2_MONK_DEAD","GLOBAL",0)
	GlobalGT("CHAMBER_2_MONK_CNT","MYAREA",0)
THEN
	RESPONSE #100
		IncrementGlobal("C2_MONK_DEAD","GLOBAL",-1)
		IncrementGlobal("CHAMBER_2_MONK_CNT","MYAREA",-1)
		Continue()
END

