        END
        READ_LONG   0xbc song_off
        PATCH_IF (~%song_off%~ >= ~%auto_map_off%~) BEGIN
          WRITE_LONG 0xbc ~%song_off%~ - ~%auto_map_num%~ * 0x34
        END
        READ_LONG   0xc0 rest_enc_off
        PATCH_IF (~%rest_enc_off%~ >= ~%auto_map_off%~) BEGIN
