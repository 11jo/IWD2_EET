        END
        READ_LONG   0xa0 explore_off
        PATCH_IF (~%explore_off%~ >= ~%auto_map_off%~) BEGIN
          WRITE_LONG 0xa0 ~%explore_off%~ - ~%auto_map_num%~ * 0x34
        END
        READ_LONG   0xa8 door_off
